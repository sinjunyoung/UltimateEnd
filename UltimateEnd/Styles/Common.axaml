	<!-- ============================================ -->
	<!-- 공통 스타일 정의 -->
	<!-- ============================================ -->
	<Style Selector=":is(TemplatedControl)">
		<Setter Property="FontFamily" Value="sans-serif"/>
	</Style>

	<Style Selector="TextBlock">
		<Setter Property="FontFamily" Value="avares://UltimateEnd/Assets/Fonts#Pretendard, Noto Color Emoji, sans-serif"/>
	</Style>
	<Style Selector="Button">
		<Setter Property="FontFamily" Value="avares://UltimateEnd/Assets/Fonts#Pretendard, Noto Color Emoji, sans-serif"/>
	</Style>
	<Style Selector="TextBox">
		<Setter Property="FontFamily" Value="avares://UltimateEnd/Assets/Fonts#Pretendard, Noto Color Emoji, sans-serif"/>
	</Style>
	<Style Selector="ComboBox">
		<Setter Property="FontFamily" Value="avares://UltimateEnd/Assets/Fonts#Pretendard, Noto Color Emoji, sans-serif"/>
	</Style>

	<Style Selector="Border.EditMode">
		<Setter Property="BorderBrush" Value="#FF6B6B"/>
		<Setter Property="BorderThickness" Value="4"/>
		<Style.Animations>
			<Animation Duration="0:0:0.5" IterationCount="Infinite">
				<KeyFrame Cue="0%">
					<Setter Property="Opacity" Value="0.8"/>
				</KeyFrame>
				<KeyFrame Cue="50%">
					<Setter Property="Opacity" Value="1.0"/>
				</KeyFrame>
				<KeyFrame Cue="100%">
					<Setter Property="Opacity" Value="0.8"/>
				</KeyFrame>
			</Animation>
		</Style.Animations>
	</Style>	
	
	<Style Selector=":is(Control):focus">
		<Setter Property="FocusAdorner" Value="{x:Null}"/>
	</Style>

	<!-- TextBlock 스타일 -->
	<Style Selector="TextBlock.description">
		<Setter Property="Foreground" Value="{DynamicResource Description.Foreground}"/>
		<Setter Property="FontStyle" Value="Normal"/>
	</Style>
	<Style Selector="TextBlock.description.placeholder">
		<Setter Property="Foreground" Value="{DynamicResource Description.PlaceholderForeground}"/>
		<Setter Property="FontStyle" Value="Italic"/>
	</Style>

	<!-- TextBox 스타일 -->
	<Style Selector="TextBox.custom-textbox">
		<Setter Property="Background" Value="Transparent"/>
	</Style>
	<Style Selector="TextBox.custom-textbox:focus">
		<Setter Property="Background" Value="Transparent"/>
	</Style>
	<Style Selector="TextBox.custom-textbox:pointerover">
		<Setter Property="Background" Value="Transparent"/>
	</Style>
	<Style Selector="TextBox:focus /template/ Border#PART_BorderElement">
		<Setter Property="Background" Value="Transparent"/>
	</Style>
		
	<!-- Button 스타일 -->
	<Style Selector="Button.menuItem">
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
		<Setter Property="HorizontalContentAlignment" Value="Left"/>
		<Setter Property="Padding" Value="15,12"/>
		<Setter Property="FontSize" Value="16"/>
		<Setter Property="Background" Value="{DynamicResource Accent.Blue}"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Red}"/>
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="Cursor" Value="Hand"/>
	</Style>

	<!-- Button 전역 스타일 -->
	<Style Selector="Button">
		<Setter Property="VerticalContentAlignment" Value="Center"/>
		<Setter Property="HorizontalContentAlignment" Value="Center"/>
	</Style>

	<Style Selector="Button /template/ ContentPresenter">
		<Setter Property="VerticalContentAlignment" Value="Center"/>
		<Setter Property="HorizontalContentAlignment" Value="Center"/>
	</Style>

	<Style Selector="Button:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource Button.Hover.Background}"/>
		<Setter Property="Foreground" Value="{DynamicResource Button.Hover.Foreground}"/>
	</Style>

	<Style Selector="Button.Primary:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="#0051D5"/>
		<Setter Property="Foreground" Value="White"/>
	</Style>

	<Style Selector="Button.Danger:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="#D32F2F"/>
		<Setter Property="Foreground" Value="White"/>
	</Style>

	<Style Selector="Button.menuItem:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource Background.Hover}"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Blue}"/>
	</Style>

	<Style Selector="Button.menuItem:pressed /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource Background.Border}"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Blue}"/>
	</Style>

	<Style Selector="Button.menuItem.selected">
		<Setter Property="Background" Value="{DynamicResource Background.Hover}"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Blue}"/>
	</Style>

	<Style Selector="Button.menuItem.selected:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource Background.Hover}"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Blue}"/>
	</Style>

	<Style Selector="Button.menuItem.danger">
		<Setter Property="Foreground" Value="{DynamicResource Accent.Red}"/>
	</Style>

	<Style Selector="Button.menuItem.danger:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource Background.Hover}"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Red}"/>
	</Style>	

	<!-- ComboBox 스타일 -->
	<Style Selector="ComboBox">
		<Setter Property="Background" Value="{DynamicResource ComboBox.Background}"/>
		<Setter Property="Foreground" Value="{DynamicResource ComboBox.Foreground}"/>
		<Setter Property="BorderBrush" Value="{DynamicResource ComboBox.Border}"/>
	</Style>
	<Style Selector="ComboBoxItem">
		<Setter Property="Background" Value="{DynamicResource ComboBox.Item.Background}"/>
		<Setter Property="Foreground" Value="{DynamicResource ComboBox.Foreground}"/>
	</Style>
	<Style Selector="ComboBoxItem:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource ComboBox.Item.Hover}"/>
	</Style>
	<Style Selector="ComboBoxItem:selected /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource ComboBox.Item.Hover}"/>
	</Style>


	<!-- PathIcon 애니메이션 스타일 -->
	<Style Selector="PathIcon.AnimatedIcon">
		<Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
		<Setter Property="Cursor" Value="Hand"/>
		<Setter Property="Foreground" Value="{DynamicResource Text.Primary}"/>
		<Setter Property="Opacity" Value="0.7"/>
		<Setter Property="RenderTransform" Value="scale(1)"/>
		<Setter Property="Transitions">
			<Transitions>
				<TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.3">
					<TransformOperationsTransition.Easing>
						<CubicEaseOut/>
					</TransformOperationsTransition.Easing>
				</TransformOperationsTransition>
				<DoubleTransition Property="Opacity" Duration="0:0:0.2"/>
				<BrushTransition Property="Foreground" Duration="0:0:0.2"/>
			</Transitions>
		</Setter>
	</Style>

	<Style Selector="PathIcon.AnimatedIcon:pointerover">
		<Setter Property="RenderTransform" Value="scale(1.2)"/>
		<Setter Property="Opacity" Value="1"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Blue}"/>
	</Style>

	<Style Selector="PathIcon.AnimatedIcon:pressed">
		<Setter Property="RenderTransform" Value="scale(0.95)"/>
		<Setter Property="Opacity" Value="0.5"/>
	</Style>

	<!-- 키보드로 메뉴 선택 시 -->
	<Style Selector="PathIcon.AnimatedIcon.MenuFocused">
		<Setter Property="RenderTransform" Value="scale(1.3)"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Blue}"/>
		<Setter Property="Opacity" Value="1"/>
	</Style>

	<!-- 호버는 여전히 작동 (우선순위 높임) -->
	<Style Selector="PathIcon.AnimatedIcon:pointerover">
		<Setter Property="RenderTransform" Value="scale(1.2)"/>
		<Setter Property="Opacity" Value="1"/>
		<Setter Property="Foreground" Value="{DynamicResource Accent.Blue}"/>
	</Style>

	<Style Selector="Border.GamepadButton">
		<Setter Property="Background">
			<Setter.Value>
				<SolidColorBrush Color="#15FFFFFF"/>
			</Setter.Value>
		</Setter>
		<Setter Property="BorderBrush" Value="#4A9EFF"/>
		<Setter Property="BorderThickness" Value="2"/>
		<Setter Property="Effect">
			<Setter.Value>
				<DropShadowEffect Color="#4A9EFF" BlurRadius="0" OffsetX="0" OffsetY="0"/>
			</Setter.Value>
